<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.dogfinder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rott_launcher"
        android:label="Dog Zone"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.root.dogfinder.ui.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.root.dogfinder.ui.DogListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.root.dogfinder.ui.MainActivity" />
        </activity>
        <activity android:name="com.example.root.dogfinder.ui.DogDetailActivity" />
        <activity android:name="com.example.root.dogfinder.ui.SavedDogListActivity"></activity>
    </application>

</manifest>